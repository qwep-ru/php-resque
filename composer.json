{
  "name" : "qwep-ru/php-resque",
  "description" : "Fork of scroller123/php-resque",
  "require" : {
    "php" : ">=5.3.0",
    "colinmollenhour/credis" : "~1.2",
    "psr/log" : "1.0.0",
    "predis/predis": "~1.0"
  },
  "repositories" : [ {
    "type" : "vcs",
    "url" : "https://github.com/chrisboulton/credis"
  } ],
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*"
  },
  "license" : "MIT",
  "keywords" : [ "job", "background", "redis", "resque" ],
  "bin" : [ "bin/resque" ],
  "autoload" : {
    "psr-0" : {
      "Resque" : "lib"
    }
  },
  "suggest" : {
    "ext-proctitle" : "Allows php-resque to rename the title of UNIX processes to show the status of a worker.",
    "ext-redis" : "Native PHP extension for Redis connectivity. Credis will automatically utilize when available."
  },
  "type" : "library",
  "homepage" : "http://www.github.com/chrisboulton/php-resque/",
  "authors" : [ {
    "name" : "Chris Boulton",
    "email" : "chris@bigcommerce.com"
  } ]
}